{
  "presets": [
    ["@babel/preset-env", {
      "loose": true
    }],
    "@babel/react"
  ],
  "plugins": [
    "@babel/transform-runtime"
  ],
  "env": {
    "production-esm": {
      "presets": [
        ["@babel/env", {
          "modules": false,
          "loose": true
        }],
        "@babel/react"
      ],
      "plugins": [
        ["@babel/transform-runtime", { "useESModules": true }]
      ]
    }
  }
}
